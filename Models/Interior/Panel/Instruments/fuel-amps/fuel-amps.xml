<?xml version="1.0"?>

<PropertyList>

 <path>fuel-amps.ac</path>

 <animation>
  <type>material</type>
  <object-name>FuelAmpsFace</object-name>
  <object-name>FuelAmpsCase</object-name>
  <object-name>FuelAmpsGlass</object-name>
  <object-name>AmpsNeedle</object-name>
  <object-name>FuelPressNeedle</object-name>
  <object-name>FuelNeedle</object-name>
  
  <emission>
   <red>1.0</red>
   <green>0.2</green>
   <blue>0.0</blue>
   <factor-prop>/sim/model//material/instruments/factor</factor-prop>
  </emission>

 </animation>
   <effect>
    <inherits-from>../../../../Effects/Glass/glass</inherits-from>
    <object-name>FuelAmpsGlass</object-name>
  </effect>


 <animation>
  <type>rotate</type>
  <object-name>FuelNeedle</object-name>
  <property>/consumables/fuel/tank[0]/level-gal_us</property>
  <interpolation>
   <entry>
    <ind>0</ind>
    <dep>-29</dep>
   </entry>
   <entry>
    <ind>1.8</ind>
    <dep>-18</dep>
   </entry>
   <entry>
    <ind>3.6</ind>
    <dep>-7</dep>
   </entry>
   <entry>
    <ind>5.4</ind>
    <dep>5</dep>
   </entry>
   <entry>
    <ind>7.2</ind>
    <dep>16</dep>
   </entry>
   <entry>
    <ind>9</ind>
    <dep>27</dep>
   </entry>
  </interpolation>
  <center>
   <x-m>0</x-m>
   <y-m>-0.039</y-m>
   <z-m>-0.022</z-m>
  </center>
  <axis>
   <x>-1</x>
   <y>0</y>
   <z>0</z>
  </axis>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>FuelPressNeedle</object-name>
  <property>/engines/engine[0]/fuel-pressure-psi</property>
  <interpolation>
   <entry>
    <ind>0</ind>
    <dep>-32</dep>
   </entry>
   <entry>
    <ind>10</ind>
    <dep>5</dep>
   </entry>
   <entry>
    <ind>18</ind>
    <dep>32</dep>
   </entry>
   <entry>
    <ind>20</ind>
    <dep>44</dep>
   </entry>
  </interpolation>
  <center>
   <x-m>0</x-m>
   <y-m>-0.002</y-m>
   <z-m>-0.014</z-m>
  </center>
  <axis>
   <x>-1</x>
   <y>0</y>
   <z>0</z>
  </axis>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>AmpsNeedle</object-name>
  <property>/systems/electrical/amps</property>
  <interpolation>
   <entry>
    <ind>-40</ind>
    <dep>-29</dep>
   </entry>
   <entry>
    <ind>-20</ind>
    <dep>-15</dep>
   </entry>
   <entry>
    <ind>0</ind>
    <dep>0</dep>
   </entry>
   <entry>
    <ind>20</ind>
    <dep>15</dep>
   </entry>
   <entry>
    <ind>40</ind>
    <dep>30</dep>
   </entry>
  </interpolation>
  <center>
   <x-m>0</x-m>
   <y-m>0.041</y-m>
   <z-m>-0.022</z-m>
  </center>
  <axis>
   <x>-1</x>
   <y>0</y>
   <z>0</z>
  </axis>
 </animation>

</PropertyList>
